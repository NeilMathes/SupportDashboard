-->
	<li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
		<div data-id="waittime" data-view="Hotness" data-title="Wait Time" data-cool="30" data-warm="55"></div>
	</li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
